# Required
VITE_MASTER_API_BASE_URL=
VITE_TELEHEALTH_LOCATION_ID=
VITE_AVAILABILITY_CATEGORY_ID=
VITE_AVAILABILITY_EVENT_ID=

# Optional authentication
VITE_MASTER_API_BEARER_TOKEN=
VITE_MASTER_API_KEY=
VITE_MASTER_API_KEY_HEADER=x-api-key

# Optional MedCard integration
# Defaults to VITE_MASTER_API_BASE_URL when empty.
VITE_MEDCARD_API_BASE_URL=
# Defaults to VITE_MASTER_API_BEARER_TOKEN when empty.
VITE_MEDCARD_BEARER_TOKEN=

# Optional payment configuration
# Procedure code used to read the service value from /master/procedures.
VITE_PAYMENT_PROCEDURE_CODE=99214

# Optional availability tuning
VITE_AVAILABILITY_DURATION_MINUTES=15
VITE_AVAILABILITY_TIME_RANGE_START=0800
VITE_AVAILABILITY_TIME_RANGE_END=1700

# Optional UI features
VITE_SHOW_SPECIALTY_SELECTOR=false

# Person (used while there is no real login)
VITE_PERSON_ID=
